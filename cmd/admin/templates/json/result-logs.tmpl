{
  "data": [
  {{range  $i, $e := .}}
    {{ if $i }}, {{ end }}
    {
      "created": {
        "display": "{{ pastTimeAgo $e.CreatedAt }}",
        "timestamp": "{{ pastTimestamp $e.CreatedAt }}"
      },
      "name": "{{ $e.Name }}",
      "columns": "{{ stringEncode $e.Columns }}"
    }
  {{end}}
  ]
}